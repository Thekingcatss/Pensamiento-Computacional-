#include <iostream>
using namespace std;

int main() {
    const int filas = 7;
    const int columnas = 35;
    int matriz[filas][columnas] = {0};

    for (int i = 0; i < filas; i++) {
        matriz[i][0] = 1;
        matriz[i][4] = 1;
    }
    for (int j = 0; j <= 4; j++) {
        matriz[3][j] = 1;
    }

    for (int i = 0; i < filas; i++) {
        matriz[i][7] = 1;
        matriz[i][11] = 1;
    }
    for (int j = 7; j <= 11; j++) {
        matriz[0][j] = 1;
        matriz[6][j] = 1;
    }

    for (int i = 0; i < filas; i++) {
        matriz[i][14] = 1;
    }
    for (int j = 14; j <= 18; j++) {
        matriz[6][j] = 1;
    }

    for (int i = 0; i < filas; i++) {
        matriz[i][21] = 1;
        matriz[i][25] = 1;
    }
    for (int j = 21; j <= 25; j++) {
        matriz[0][j] = 1;
        matriz[3][j] = 1;
    }

    for (int i = 0; i < filas; i++) {
        for (int j = 0; j < columnas; j++) {
            if (matriz[i][j] == 1)
                cout << "*";
            else
                cout << " ";
        }
        cout << endl;
    }

    return 0;
}

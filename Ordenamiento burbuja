#include <iostream>
using namespace std;

int main() {
    int arreglo[5];
    
    cout << "Ingrese 5 numeros:" << endl;
    for (int i = 0; i < 5; i++) {
        cin >> arreglo[i];
    }

    for (int i = 0; i < 5 - 1; i++) {
        for (int j = 0; j < 5 - i - 1; j++) {
            if (arreglo[j] > arreglo[j + 1]) {
                int temp = arreglo[j];
                arreglo[j] = arreglo[j + 1];
                arreglo[j + 1] = temp;
            }
        }
    }

    cout << "Numeros ordenados:" << endl;
    for (int i = 0; i < 5; i++) {
        cout << arreglo[i] << " ";
    }
    cout << endl;

    return 0;
}

#include <iostream>
using namespace std;
int main() {
    int matriz[6][6];
    int numero = 2; 
    for (int i = 0; i < 6; i++) {
        for (int j = 0; j < 6; j++) {
            if (j % 2 == 1) { 
                matriz[i][j] = 0;
            } else { 
                matriz[i][j] = numero; 
                numero += 2; 
            }
             cout<<matriz[i][j]<< " ";
        }
        cout<<endl;
    }
    return 0;
}
